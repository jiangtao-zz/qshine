<?xml version="1.0" encoding="utf-8"?>
<!--Configure for SqlServer -->
<configuration>
  <configSections>
    <section name="qshine" type="qshine.Configuration.EnvironmentSection, qshine" />
  </configSections>
  <qshine>
    <!--qshine configuration setting folder path. ./Config is the default folder.
		the framewrok will load all configuration files from this folder.
		-->
    <!--Install SqlServer DDL provider-->
    <components>
      <component name="dialect" interface="qshine.database.ISqlDialectProvider" type="qshine.database.sqlserver.SqlDialectProvider, qshine.database.sqlserver"/>
    </components>
  </qshine>

  <connectionStrings>
    <add name="testdb" providerName="System.Data.SqlClient" connectionString="Data Source=(LocalDb)\sampledbinstance;Initial Catalog=sampledb;Integrated Security=True" />
  </connectionStrings>

</configuration>
