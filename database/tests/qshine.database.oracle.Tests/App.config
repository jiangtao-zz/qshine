<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="qshine" type="qshine.Configuration.EnvironmentSection, qshine"/>
  </configSections>
  <qshine>
    <!--qshine configuration setting folder path. ./Config is the default folder.
	the framewrok will load all configuration files from this folder.
	-->
    <environments>
      <environment name="oracle" path="config/oracle" bin="bin"/>
    </environments>    
  </qshine>

  <system.data>
    <!--Sqlite provider-->
    <DbProviderFactories>
      <!--Oracle provider-->
      <remove invariant="Oracle.ManagedDataAccess.Client"/>
      <add name="ODP.NET, Managed Driver" invariant="Oracle.ManagedDataAccess.Client" description="Oracle Data Provider for .NET, Managed Driver" type="Oracle.ManagedDataAccess.Client.OracleClientFactory, Oracle.ManagedDataAccess, Version=4.122.18.3, Culture=neutral, PublicKeyToken=89b483f429c47342"/>

    </DbProviderFactories>
  </system.data>

  <system.diagnostics>
    <sources>
      <source name="dev" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="console"/>
          <remove name="Default"/>
        </listeners>
      </source>

      <source name="General" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="console"/>
          <remove name="Default"/>
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="sourceSwitch" value="All"/>
    </switches>
    <sharedListeners>
      <add name="console" type="System.Diagnostics.ConsoleTraceListener">
      </add>
    </sharedListeners>

  </system.diagnostics>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
</configuration>

