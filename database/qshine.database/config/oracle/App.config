<?xml version="1.0" encoding="utf-8"?>
<!--Configure for Oracle -->
<configuration>
	<configSections>
		<section name="qshine" type="qshine.Configuration.EnvironmentSection, qshine" />
    <section name="oracle.manageddataaccess.client"
      type="OracleInternal.Common.ODPMSectionHandler, Oracle.ManagedDataAccess, Version=4.122.18.3, Culture=neutral, PublicKeyToken=89b483f429c47342"/>
  </configSections>
	<qshine>
		<!--qshine configuration setting folder path. ./Config is the default folder.
		the framewrok will load all configuration files from this folder.
		-->
		<!--Install Oracle Dialect provider-->
		<components>
			<component name="sqlDialect" interface="qshine.database.ISqlDialectProvider" type="qshine.database.oracle.SqlDialectProvider, qshine.database.oracle"/>
		</components>
	</qshine>

  <connectionStrings>
    <add name="testDatabase" providerName="Oracle.ManagedDataAccess.Client" connectionString="user id=sampledb;password=royal1;data source=sampledb" />
  </connectionStrings>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <publisherPolicy apply="no"/>
        <assemblyIdentity name="Oracle.ManagedDataAccess" publicKeyToken="89b483f429c47342" culture="neutral"/>
        <bindingRedirect oldVersion="4.122.0.0 - 4.65535.65535.65535" newVersion="4.122.18.3"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <oracle.manageddataaccess.client>
    <version number="*">
      <dataSources>
        <dataSource alias="sampledb" descriptor="(DESCRIPTION=(ADDRESS=(PROTOCOL=tcp)(HOST=localhost)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=XE)))"/>
      </dataSources>
    </version>
  </oracle.manageddataaccess.client>  

</configuration>
