<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<!-- qshine environment config section -->
		<section name="qshine" type="qshine.Configuration.EnvironmentSection, qshine" />
	</configSections>
	
	<qshine>
		<!--plug-in database component -->
		<components>
			<!--
				plug-in database SQL syntax dialect component.
			-->
			<!--mysql component-->
			<component name="mysqlDialect" interface="qshine.database.ISqlDialectProvider" type="qshine.database.mysql.SqlDialectProvider, qshine.database.mysql"/>
		</components>
	</qshine>
</configuration>
