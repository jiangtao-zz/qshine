<?xml version="1.0" encoding="utf-8"?>
<!--Configure for MySql -->
<configuration>
	<configSections>
		<section name="qshine" type="qshine.Configuration.EnvironmentSection, qshine" />
	</configSections>
	<qshine>
		<!--qshine configuration setting folder path. ./Config is the default folder.
		the framewrok will load all configuration files from this folder.
		-->
		<!--Install MySql DDL provider-->
		<components>
			<component name="sqlSyntax" interface="qshine.database.ISqlDialectProvider" type="qshine.database.mysql.SqlDialectProvider, qshine.database.mysql"/>
		</components>
	</qshine>

  <connectionStrings>
    <add name="testDatabase" providerName="MySql.Data.MySqlClient" connectionString="Server=localhost;Database=sampledb;Uid=dev;Pwd=royal1;Port=3306;SslMode=none" />
  </connectionStrings>  

</configuration>
