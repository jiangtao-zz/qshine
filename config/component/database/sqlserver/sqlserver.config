<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<!-- qshine environment config section -->
		<section name="qshine" type="qshine.Configuration.EnvironmentSection, qshine" />
	</configSections>
	
	<qshine>
		<!--
			plug-in database SQL syntax dialect component.
		-->
		<!--sql server component-->		
		<components>
			<component name="sqlserverDialect" interface="qshine.database.ISqlDialectProvider" type="qshine.database.sqlserver.SqlDialectProvider, qshine.database.sqlserver"/>
		</components>
	</qshine>

	<connectionStrings>
		<add name="testDatabase" providerName="System.Data.SqlClient" connectionString="Data Source=(LocalDb)\sampledbinstance;Initial Catalog=sampledb;Integrated Security=True" />
	</connectionStrings>  

</configuration>
